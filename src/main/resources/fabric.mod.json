{
  "schemaVersion": 1,
  "id": "bewisclient",
  "version": "${version}",
  "name": "Bewisclient",
  "description": "Bewisclient adds many quality of life features to Minecraft.",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC-BY 4.0",
  "icon": "assets/bewisclient/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "value": "net.bewis09.bewisclient.BewisclientInitializer",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "net.bewis09.bewisclient.util.BewisclientDataGenerator",
        "adapter": "kotlin"
      }
    ],
    "bewisclient": [
      {
        "value": "net.bewis09.bewisclient.impl.BewisclientSelfAPIEntrypoint",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    {
      "config": "bewisclient.client.mixins.json",
      "environment": "client"
    },
    {
      "config": "bewisclient.independent.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "${version_range}",
    "java": ">=21",
    "fabric-api": "*",
    "fabric-language-kotlin": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}